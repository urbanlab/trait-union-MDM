<link href="<%= BASEURL %>/assets/css/normalize.css" rel='stylesheet' type='text/css'>
<link href="<%= BASEURL %>/assets/css/css.css" rel='stylesheet' type='text/css'>

<!--<%= floor %>-->
<body id="style">

<div class="head-image"><img class="titre" src="../assets/img/ambiance.png" alt="quelle ambiance ?!"></div>

<div class="aligner">
<div class="mood" data-value="relax" data-color="#C5FFE1"><img class="pictos" src="../assets/img/relax.png" alt="relax"></div>
<div class="mood" data-value="classique" data-color="#C2CCFF"><img class="pictos" src="../assets/img/classique.png" alt="classique"></div>
<div class="mood" data-value="swing" data-color="#FF4800"><img class="pictos" src="../assets/img/swing.png" alt="swing"></div>
<div class="mood" data-value="disco" data-color="#F82AF8"><img class="pictos" src="../assets/img/disco.png" alt="disco"></div>
<div class="mood" data-value="energie" data-color="#FFFF06"><img class="pictos" src="../assets/img/energie.png" alt="energie"></div>
</div>

<div class="texte">
<p class="moyenne">En ce moment, <br>
 vous êtes tou·te·s très swing !</p>
<p class="ailleurs">Comment ça se passe dans chaque MDM ?</p>
</div>

<hr>

<div class="reactive-zone">
  <div class="cols">

    <div class="topmood">
      <%= stats.moyMood.mood %>
    </div>

    <div class="floors">

      <% for (var floorIndex = 0; floorIndex < stats.floors.length; floorIndex++) { %>

        <div class="floor">
          <div class="floor-index">
            <%= floorIndex %>
          </div>

          <div class="floor-mood">
            <%= stats.floors[floorIndex].topMood.mood %>
          </div>
        </div>

      <% } %>
    </div>

  </div>
</div>






<script>
  var BASEURL = '<%= BASEURL %>';
  var floor = '<%= floor %>';
</script>

<script src="<%= BASEURL %>/assets/lib/jquery-2.2.4.min.js"></script>
<script src="<%= BASEURL %>/assets/lib/socket.io-1.2.0.js"></script>
<script src="<%= BASEURL %>/assets/js/js.js"></script>

</body>
